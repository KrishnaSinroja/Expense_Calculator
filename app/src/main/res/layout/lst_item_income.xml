<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:id="@+id/inclvchild"
    android:layout_width="fill_parent"
    android:layout_height="85dip"
    android:layout_margin="5dp">

    <TextView
        android:id="@+id/IncomelblListCategory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18dp"

        android:layout_marginTop="10dp"
        android:paddingLeft="10dp"/>

    <TextView
        android:id="@+id/IncomelblListDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:textSize="15dip"
        android:layout_marginTop="10dp"
        android:paddingLeft="10dp"
        android:layout_below="@+id/IncomelblListCategory"/>

    <TextView
        android:id="@+id/IncomelblListIncome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="170dp"
        android:layout_toRightOf="@+id/IncomelblListDate"/>


    <TextView
        android:id="@+id/IncomelblListid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"
        />


</RelativeLayout>